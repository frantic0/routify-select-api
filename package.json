{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@sveltech/routify": "^1.0.1-7",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "serve": "^11.0.0",
    "spassr": "^1.0.2",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "run-p watch:routify watch:webpack",
    "watch:webpack": "webpack-dev-server --content-base public",
    "watch:routify": "routify -d --pages ./client/pages",
    "serve": "spassr --debug --serve-spa --spa-port 5001 --dist-dir ./public --app ./bundle.js --entrypoint ./index.html",
    "deploy:now": "cd scripts/now && npm run deploy"
  },
  "dependencies": {}
}
